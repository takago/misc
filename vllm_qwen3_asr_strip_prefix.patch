+# optional: Qwen3-ASR helper
+try:
+    from qwen_asr import parse_asr_output as _parse_asr_output
+except Exception:
+    _parse_asr_output = None
+
+_QWEN_ASR_PREFIX_RE = re.compile(r"^\s*language\s+[^<]*<asr_text>\s*", re.IGNORECASE)
+
+def _strip_qwen_asr_prefix(s: str) -> str:
+    if not s:
+        return s
+    # prefer the official parser when available
+    if _parse_asr_output is not None:
+        try:
+            _lang, _text = _parse_asr_output(s)
+            return _text
+        except Exception:
+            pass
+    # fallback heuristic
+    m = _QWEN_ASR_PREFIX_RE.match(s)
+    return s[m.end():] if m else s
